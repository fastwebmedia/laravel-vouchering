{
    "name": "fastwebmedia/laravel-vouchering",
    "description": "A simple vouchering system for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Fast Web Media Ltd",
            "email": "packages@fastwebmedia.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.1.*",
        "rhumsaa/uuid": "2.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "league/factory-muffin": "~3.0@dev",
        "league/factory-muffin-faker": "~1.0@dev",
        "illuminate/database": "4.1.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/Models",
            "src/commands",
            "src/Factories",
            "src/Repositories",
            "src/Facades"
        ],
        "psr-4": {
            "Fastwebmedia\\LaravelVouchering\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/AbstractFWMTestCase.php"
        ]
    }
}
